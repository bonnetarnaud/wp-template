{
    "name": "arnaudbonnet/debug-symfony-plugin",
    "description": "WordPress must-use plugin for doing stuff",
    "license": "GPL-2.0+",
    "type": "wordpress-muplugin",
    "require": {
        "symfony/var-dumper": "^5.1"
    },
    "config": {
        "preferred-install": "dist",
        "vendor-dir": "wp-content/mu-plugins/vendor"
    },
    "extra": {
        "installer-paths": {
            "mu-plugins/{$name}/": ["type:wordpress-muplugin"]
        }
    }
}
